{
  "name": "Eza (via Github Releases)",
  "id": "eza",
  "version": "1.1.1",
  "description": "A feature to install [Eza](https://github.com/eza-community/eza)",
  "options": {
    "version": {
      "default": "latest",
      "description": "Select the version to install.",
      "proposals": [
        "latest"
      ],
      "type": "string"
    }
  },
  "dependsOn": {
    "ghcr.io/devcontainers-extra/features/gh-release": {}
  },
  "installsAfter": [
    "ghcr.io/devcontainers-extra/features/gh-release"
  ]
}
